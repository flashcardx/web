      <div class="row">
        <div class="col">
                            <ul class="list-inline">  
                                <li class="list-inline-item"><a href="/discover">Cards</a></li>
                                <li class="list-inline-item"><a href="#"><u>Classes</u></a></li>
                            </ul>
        </div>
      </div>
      <div style="margin-top:0px;" class="class-group">
                <div class="input-group row">
                              <input id="search-box" type="search" class="form-control input-lg"
                                  placeholder="Search class" />
                              <span class="input-group-btn">
                                          <button id ="search-button" class="btn btn-success" role="button" type="button" onClick="searchClass();">
                                              <i class="fa fa-search fa-fw" aria-hidden="true"></i>
                                          </button>
                              </span>
                </div>
              <div class="row">
                  <div class="col container" id="search-results">
                  </div>
              </div>
        </div>




        <div class="row">
          <div class="col classes">
              <div id="discover-classes-content" class="container">
              </div>
          </div>
        </div>


         <%- include('tiles/modalActivity'); %>

         <!--modal classes list-->
          <div class="modal fade" id="duplicate-on-class-modal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog" role="document">
              <div class="modal-content">
                <div class="modal-header">
                  <h5 class="modal-title" id="exampleModalLabel">Select class to duplicate card:</h5>
                  <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
              </button>
                </div>
                <div class="modal-body">
                    <select name="class" class="form-control filter-margin" id="class-select">
                    </select>
                </div>
                <div class="modal-footer">
                  <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                  <button type="button" onCLick="duplicate2Class();" data-dismiss="modal" class="btn btn-primary">Confirm</button>
                </div>
              </div>
            </div>
          </div>


    </div>
