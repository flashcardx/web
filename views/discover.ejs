<div class="main-content  min-height">
  <div class=" container ">
    <% errors.forEach(err=>{%>
      <div class="alert alert-danger alert-dismissable">
        <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
        <strong>Error!</strong>
        <%= err%>
      </div>
      <%});%>
      <div class="row">
        <div class="col">
                            <ul class="list-inline">  
                                <li class="list-inline-item"><a href="#"><u>Cards</u></a></li>
                                <li class="list-inline-item"><a href="/discoverClasses">Classes</a></li>
                            </ul>
        </div>
      </div>

        <div class="row ev-pd-top">
          <div class="col-md-12 col-12">
            <div class="row"id="card-deck">
              <!--Here in Jquery-->
            </div>
          </div>
        </div>


         <%- include('tiles/modalActivity'); %>

         <!--modal classes list-->
          <div class="modal fade" id="duplicate-on-class-modal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog" role="document">
              <div class="modal-content">
                <div class="modal-header">
                  <h5 class="modal-title" id="exampleModalLabel">Select class to duplicate card:</h5>
                  <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
              </button>
                </div>
                <div class="modal-body">
                    <select name="class" class="form-control filter-margin" id="class-select">
                    </select>
                </div>
                <div class="modal-footer">
                  <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                  <button type="button" onCLick="duplicate2Class();" data-dismiss="modal" class="btn btn-primary">Confirm</button>
                </div>
              </div>
            </div>
          </div>


    </div>
  </div><!--end main container -->

  <%- include('tiles/footer'); %>

    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
