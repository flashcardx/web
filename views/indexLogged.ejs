            <a class="nav-link" href="/practice">Practice</a>
          </li>
        </ul>
          <%- include('tiles/sideOptions'); %>
          </div>
    </div>
  </nav>
  <div class="main-content min-height">
          <div class="container">
            <% errors.forEach(err=>{%>
              <div class="alert alert-danger alert-dismissable">
                <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                <strong>Error!</strong>
                <%= err%>
              </div>
              <%});%>

                <div class="row">
                  <div class="col-md-2"></div>
                  <div class="col-md-8">
                    <div class="row">
                      <div class="col-md-4 filters">
                        <div class="filter-container">
                        <!-- <span>  Sort by:</span> -->
                          <select name="sort" class="form-control filter-margin" id="sort-select">
                                <option selected="selected" value="desc">Newer first</option>
                                <option value="asc">Older first</option>
                            </select>
                        </div>
                      </div>
                      <div class="col-md-4 filters">
                        <div class="filter-container">
                          <!-- <span>  Category:</span> -->
                          <select name="category" class="form-control filter-margin" id="category-select">
                            </select>
                        </div>
                      </div>
                      <div class="col-md-4">
                        <div class="filter-container">
                          <!--<span>  Search card's title:</span> -->
                          <input id="search-box" type="search" class="form-control filter-margin" placeholder="card's title" />
                        </div>
                      </div>
                    </div>
                  </div>
                </div>


                <div class="row ev-pd-top">
                  <div class="col-md-12">
                    <div class="row" id="card-deck">
                    </div>
                  </div>

                </div>

                <%- include('tiles/modalActivity'); %>

                <!--modal classes list-->
          <div class="modal fade" id="duplicate-on-class-modal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog" role="document">
              <div class="modal-content">
                <div class="modal-header">
                  <h5 class="modal-title" id="exampleModalLabel">Select class to duplicate card:</h5>
                  <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
              </button>
                </div>
                <div class="modal-body">
                    <select name="class" class="form-control filter-margin" id="class-select">
                    </select>
                </div>
                <div class="modal-footer">
                  <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                  <button type="button" onCLick="duplicate2Class();" data-dismiss="modal" class="btn btn-primary">Confirm</button>
                </div>
              </div>
            </div>
          </div>

                <!--Modal new category-->
                <div class="modal fade" id="new-category-modal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                  <div class="modal-dialog" role="document">
                    <div class="modal-content">
                      <div class="modal-header">
                        <h5 class="modal-title" id="exampleModalLabel">New category</h5>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                      <span aria-hidden="true">&times;</span>
                    </button>
                      </div>
                      <div class="modal-body">
                        <input type="text" class="form-control" id="new-category" placeholder="Category's name">
                      </div>
                      <div class="modal-footer">
                        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                        <button type="button" onCLick="newCategory()" data-dismiss="modal" class="btn btn-primary">Add category</button>
                      </div>
                    </div>
                  </div>
                </div>

          </div> <!-- end container-->
    
  </div> <!-- end main-content-->
  <%- include('tiles/footer'); %>



