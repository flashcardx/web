<div class="card">
                                <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
                  <div class="carousel-inner" role="listbox">
                      <%card.imgs.forEach(function(img, index){
                        %>trhr5yth<%  if(index === 0){%>fgrfg
                            <div class="carousel-item active">
                                <img class="d-block img-fluid slide-size" src="<%= img %>" alt="First slide">
                            </div>

                          <%}
                      });%>
                      
                      <div class="carousel-item">
                        <img class="d-block img-fluid slide-size" src="http://www.thewayofmeditation.com.au/wp-content/uploads/2015/12/breath.jpg" alt="Second slide">
