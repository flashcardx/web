      <% if (!data.success) { %>
    <div class="alert alert-danger">
      <strong>Error!</strong> <%= data.msg%>
  </div>  
 <% }%>
  <div class= " main-content">
   

    <div class="card-deck">
                   <%if(data.success){
        data.msg.forEach(function(card, index1){%>
          <%- include tiles/card %>
       <% });
    }%>
       
      
    </div>
    <!-- end card-deck-->
  </div>
  <!-- end container-->




